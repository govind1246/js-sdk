{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-classes-per-file": false,
    "max-line-length": false,
    "interface-name": false,
    "class-name": false,
    "variable-name": false,
    "no-console": false,
    "one-variable-per-declaration": false
  },
  "linterOptions": {
    "exclude": [
      "src/samples/*.ts",
      "src/test/**/*.ts"
    ]
  },
  "rulesDirectory": []
}
